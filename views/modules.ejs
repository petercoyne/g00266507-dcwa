<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Modules</title>
</head>
<body>
	<h1>Modules</h1>
	<a href="/"><- Home</a><br/>
	<table>
		<tr>
			<th>ID</th>
			<th>Name</th>
			<th>Credits</th>
			<th>Edit</th>
			<th>List</th>
		</tr>
		<% modules.forEach((module) => { %>
			<tr>
				<td><%= module.mid %></td>
				<td><%= module.name %></td>
				<td><%= module.credits %></td>
				<td><a href="/modules/<%= module.mid %>">Edit</a></td>
				<td><a href="/module/students/<%= module.mid %>">List</a></td>
			</tr>
		<% }) %>
	</table>
</body>
</html>